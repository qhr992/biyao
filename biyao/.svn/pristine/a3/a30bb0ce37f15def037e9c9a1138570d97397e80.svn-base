<script>

    var left_seconds = 7012

    var intcount;

    $(function(){
        //延长收货
        if($('.ycsh_djs').length>0){
            var clt=$('.ycsh_djs').attr('clts');
            var formatTime=function(mstime){
                var day = parseInt(mstime/60/60/24);
                var hour = parseInt(mstime/60/60%24);
                var min = parseInt(mstime/60%60);
                var sec = parseInt(mstime%60);
                return '还剩 '+day+'天'+hour+"时"+min+"分"+sec+"秒";
            }
            var djs=setInterval(function(){
                if(clt>0){
                    $('.ycsh_djs').html(formatTime(clt));
                    clt=clt-1;
                }else{
                    window.location.reload();
                }
            },1000);
            $('.ycshck').click(function(){
                var orderid=$(this).attr('orderid')
                if(clt>3*24*60*60){
                    LT.alertSmall('您可以在离超时结束还剩最后<span class="col_f60">3</span>天时，申请延长确认收货时间！');
                    $('.J_pop').css('width','400px');
                }else if(clt>0&&clt<=3*24*60*60){
                    LT.confirmSmall('您可以延长该订单收货时间，将有更多的时间进行"确认收货"，延长天数为<span class="col_f60">5</span>天。<br><span class="col_link">提示：收货时间只能延长一次</span>', function (data) {
                        if (data == true) {
                            $.ajax({
                                type:"post",
                                url:"/MyCenter/ycsh",
                                data:{orderId:orderid},
                                success:function(data){
                                    if(data.success == 1){
                                        LT.alertSmall('您已成功将订单的收货时间延长<span class="col_f60">5</span>天！<br><span class="col_link">提示：如果您还是没收到货，请联系商家</span>',function(){window.location.reload();});
                                    }else{LT.alertSmall('网络连接失败，请稍候重试');}},
                                error:function(data){LT.alertSmall('网络连接失败，请稍候重试');}
                            });
                        }
                    });
                    $('.J_pop').css('width','470px');
                }
            });
        }
        //促销相关
        $(".msgBtn").bind('click',function(){
            var self = $(this);
            var id = self.attr("dataid");
            //预售
            if(self.attr("datatype") == 2){
                var preSaleDiv = "#preSaleDiv"+id;
                //显示价格
                var dataprice= parseFloat(self.attr("dataprice"));
                $("td[name='promotionPrice']").each(function(index){
                    var td = $(this);
                    var discount = td.attr("discount");
                    td.html(dataprice*parseFloat(discount)/100.0);
                });
                $(preSaleDiv).show();
                $(preSaleDiv+" .pop_close,#iknow").unbind("click").bind("click",function(){
                    $(preSaleDiv).hide();
                });
            }//分享免邮
            else{
                var sharePromotion = "#sharePromotion" + id;
                $(sharePromotion).show();
                $(sharePromotion+" .pop_close").unbind("click").bind("click",function(){
                    $(sharePromotion).hide();
                });
            }
        });
        $('.wl_dl_w').mask(true);
        $.post(window.ControllerSite+'/SfExpress/QueryExpressInfo','tarcking_number='+$('#wl_order').html() + '&orderId='+ $("[data-orderid]").attr("data-orderid"),function(wl_data){
            var str = "";
            try{
                var jsons = eval('(' + wl_data.data + ')');
                for(var i=0;i<jsons.length;i++)
                {
                    str = '<div class="wl_dl"><span class="w150">'+jsons[i].accept_time+'</span><span class="w650">'+jsons[i].remark+'</span></div>' + str;
                }
                $('.wl_dl_w').html(str);
                if (jsons != null && jsons.length > 0) {
                    if (jsons[jsons.length-1].opcode == 3) {
                        $('.progress_box').html('<span class="inline active" style="width:' + jsons[jsons.length-1].rate + '%"></span>');
                        $('.progress_box').next().html(jsons[jsons.length-1].rate + '%&nbsp;&nbsp;&nbsp;' + jsons[jsons.length-1].processname);
                    }
                }
            }catch(e){
                $('.wl_dl_w').css('background-image','none').html('<div class="pd_t10 pd_b10" align="left">获取信息错误  . . .<div>');
            }
        })

        if (order_status == 1){
            if (left_seconds > 0) {
                intcount = self.setInterval("count()",1000);
            } else {
                self.clearInterval(intcount);
                $("#left_ul").remove();
                $(".orderdetails_box1 p:first").empty();
                $(".orderdetails_box1 p:first").append("<span class='inline'><span class='col_666 f18'>当前订单状态：</span> <span class='col_f60 f18'>已关闭！</span></span>");
                $(".orderdetails_box1 p:last").empty();
                $(".orderdetails_box1 p:last").append("<span class='f14 col_666'>取消原因：超时未付款，自动取消订单</span><span class='f14 col_666'></span>");
            }
        }

    })

    function count(){
        left_seconds--;
        if (left_seconds < 0) {
            self.clearInterval(intcount);
            $("#left_ul").remove();
            $(".orderdetails_box1 p:first").empty();
            $(".orderdetails_box1 p:first").append("<span class='inline'><span class='col_666 f18'>当前订单状态：</span> <span class='col_f60 f18'>已关闭！</span></span>");
            $(".orderdetails_box1 p:last").empty();
            $(".orderdetails_box1 p:last").append("<span class='f14 col_666'>取消原因：超时未付款，自动取消订单</span><span class='f14 col_666'></span>");
        } else {
            var left_hour = Math.floor(left_seconds/3600);
            var left_minute = Math.floor(left_seconds/60)%60;
            $("#left_time").html(left_hour+ "小时"+ left_minute + "分钟后");
        }
    }
</script>




