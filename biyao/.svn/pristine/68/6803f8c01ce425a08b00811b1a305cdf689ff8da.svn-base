package com.cmhzteam.entity.lyp;

import java.util.List;

/**
 * 商品详情主属性表
 */
public class GoodMainParam {
    private int gmpid;//主属性id
    private int gid;//商品id
    private String mparam_name;//主属性名称
    private double add_price;//加价
    private int stock;//库存

    private List<GoodSecondParam> gspList;//从属性


    public GoodMainParam() {
    }

    public GoodMainParam(int gmpid, int gid, String mparam_name, double add_price, int stock, List<GoodSecondParam> gspList) {
        this.gmpid = gmpid;
        this.gid = gid;
        this.mparam_name = mparam_name;
        this.add_price = add_price;
        this.stock = stock;
        this.gspList = gspList;
    }

    public int getGmpid() {
        return gmpid;
    }

    public void setGmpid(int gmpid) {
        this.gmpid = gmpid;
    }

    public int getGid() {
        return gid;
    }

    public void setGid(int gid) {
        this.gid = gid;
    }

    public String getMparam_name() {
        return mparam_name;
    }

    public void setMparam_name(String mparam_name) {
        this.mparam_name = mparam_name;
    }

    public double getAdd_price() {
        return add_price;
    }

    public void setAdd_price(double add_price) {
        this.add_price = add_price;
    }

    public int getStock() {
        return stock;
    }

    public void setStock(int stock) {
        this.stock = stock;
    }

    public List<GoodSecondParam> getGspList() {
        return gspList;
    }

    public void setGspList(List<GoodSecondParam> gspList) {
        this.gspList = gspList;
    }

    @Override
    public String toString() {
        return "GoodMainParam{" +
                "gmpid=" + gmpid +
                ", gid=" + gid +
                ", mparam_name='" + mparam_name + '\'' +
                ", add_price=" + add_price +
                ", stock=" + stock +
                ", gspList=" + gspList +
                '}';
    }
}
